@model CQA.Models.Menu
<!-- Start: Navigation wrapper -->
        <div class="navbar navbar-fixed-top">
            <div class="navbar-inner">
                <div class="container" style="height:auto; padding:0;">
                    
                    <!-- Below button used for responsive navigation -->
                    <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>

                    <div class="nav-collapse collapse">
                        <ul class="nav pull-left">
                            @foreach(var s in Model.Setups){
                            <li>@Html.ActionLink(s.Subject.Shortcut + " " + s.Name, "AnswerAndEvaluate", "Questions", new { setupId = s.SetupId }, new { id = "MenuSetupItem" + s.SetupId }) </li>
                            }
                        </ul>
                    </div>
                    <!-- Start: Primary navigation -->
                    <div class="nav-collapse collapse">
                        <ul class="nav pull-right">
                            <li>@Html.ActionLink("Predmety", "Index", "Subjects")</li>
                            @if(Model.SubjectsNotEmpty){
                                <li>@Html.ActionLink("Setups", "Index", "Setups")</li>
                            }
                             @Html.Partial("_LoginPartial")
                        </ul>

                    </div>
                </div>
            </div>
        </div>
        <!-- End: Navigation wrapper -->

